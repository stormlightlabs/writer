{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for main and quick capture windows",
  "windows": ["main", "quick_capture"],
  "permissions": [
    "core:default",
    "log:default",
    "opener:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:scope",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    "fs:read-all",
    "fs:write-files",
    "store:default"
  ]
}
